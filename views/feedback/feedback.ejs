<%- include('../inc/top'); %>
  <div class="container">
    <main class="mt-5 pt-3">
      <div class="row">
        <div class="col-md-12">
          <h2 style="text-align: center; margin-bottom: 65px; color: darkblue;">FEEDBACK</h2>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 mb-3">
          <div class="card">
            <div class="card-body">
              <div class="table-responsive">
                <table id="example" class="table table-striped data-table" style="width: 100%">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Phone</th>
                      <th>Email</th>
                      <th>Comment</th>
                      <th>Image</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% list.forEach(data=>{%>
                      <tr>
                        <td>
                          <%=data.name %>
                        </td>
                        <td>
                          <%=data.phone %>
                        </td>
                        <td>
                          <%=data.email %>
                        </td>
                        <td>
                          <%=data.comment %>
                        </td>
                        <td><img style="width: 80px; height: 75px;" src="<%= data.image %>"></td>
                      </tr>

                      <%} ) %>

                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>
  </main>
  </div>
  <%- include('../inc/bottom'); %>

    </html>